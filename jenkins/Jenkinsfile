pipeline {
    agent any

    node {
        checkout scm
        def customImage = docker.build("cell-img:${env.BUILD_ID}")
    }
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
